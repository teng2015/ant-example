{
  "name": "antd-demo",
  "version": "1.0.0",
  "private": true,
  "entry": {
    "index": "./src/entry/index.jsx",
    "demo": "./src/entry/demo.jsx"
  },
  "dependencies": {
    "antd": "0.12.x",
    "atool-build": "0.6.x",
    "babel-plugin-antd": "0.3.x",
    "es3ify-loader": "^0.1.0",
    "react": "0.14.x",
    "react-dom": "0.14.x"
  },
  "devDependencies": {
    "atool-doc": "~0.2.0",
    "babel-eslint": "^6.0.0",
    "dora": "0.3.x",
    "dora-plugin-hmr": "0.5.x",
    "dora-plugin-livereload": "0.3.x",
    "dora-plugin-proxy": "0.6.x",
    "dora-plugin-webpack": "0.6.x",
    "eslint": "2.x",
    "eslint-config-airbnb": "6.x",
    "eslint-plugin-react": "4.x",
    "pre-commit": "1.x",
    "webpack": "~1.12.15"
  },
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "npm run dev",
    "dev": "dora -p 8001 --plugins webpack,hmr,proxy,livereload?enableJs=false",
    "lint": "eslint --ext .js,.jsx src",
    "build": "atool-build -o ./dist/${npm_package_family}/${npm_package_name}/${npm_package_version}",
    "doc-build": "atool-doc --build",
    "doc": "atool-doc"
  }
}
